#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sagar/project/hackthon/useDApp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/sagar/project/hackthon/useDApp/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/ts-node@10.7.0_46pgf7sfaob72lkbqjt5y5pgiu/node_modules/ts-node/dist/bin-esm.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/ts-node@10.7.0_46pgf7sfaob72lkbqjt5y5pgiu/node_modules/ts-node/dist/bin-esm.js" "$@"
fi
